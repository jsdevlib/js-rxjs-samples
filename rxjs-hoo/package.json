{
  "name": "ts-node-starter",
  "version": "1.0.0",
  "description": "A simple template starter for nodejs with typescript",
  "main": "index.js",
  "scripts": {
    "start:dev": "npx nodemon",
    "build": "rimraf ./dist && tsc -p tsconfig.build.json",
    "start": "npm run build && node build/index.js",
    "lint": "eslint src/**/*.ts",
    "format": "eslint src/**/*.ts --fix",
    "test": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/beyondnetPeru/ts-node-starter.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/beyondnetPeru/ts-node-starter/issues"
  },
  "homepage": "https://github.com/beyondnetPeru/ts-node-starter#readme",
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "@types/node": "^18.14.0",
    "jest": "^29.4.3",
    "nodemon": "^2.0.20",
    "rimraf": "^4.1.2",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "eslint": "^8.34.0",
    "prettier": "^2.8.4",
    "rxjs": "^7.8.0"
  }
}
